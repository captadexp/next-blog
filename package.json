{
  "name": "@supergrowthai/next-blog",
  "version": "1.0.13",
  "description": "Add blogging to your nextjs project in a jiffy",
  "repository": "https://github.com/captadexp/next-blog",
  "author": "Capt ADExp <tech@developer.toys>",
  "license": "ISC",
  "scripts": {
    "dev": "webpack serve --mode=development",
    "build": "yarn clean && yarn tsc && webpack --mode=production",
    "webpack-build": "webpack --mode=production",
    "clean": "rm -rf dist",
    "start": "node dist/render/index.js"
  },
  "keywords": [
    "nextjs",
    "react",
    "cms",
    "blogging",
    "headless-cms",
    "next-js",
    "secure"
  ],
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./adapters/*": {
      "types": "./dist/adapters/*.d.ts",
      "import": "./dist/adapters/*.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "next": "^15.2.2"
  },
  "dependencies": {
    "moment": "^2.30.1",
    "preact": "^10.26.4",
    "preact-render-to-string": "^6.5.13"
  },
  "optionalDependencies": {
    "mongodb": "^6.14.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-build-classic": "^44.3.0",
    "@types/mongodb": "^4.0.7",
    "@types/next": "^9.0.0",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.8.2",
    "@swc/register": "^0.1.10"
  }
}
