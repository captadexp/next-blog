{
  "name": "@supergrowthai/oneapi",
  "version": "2.0.1-beta.1",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./express": {
      "types": "./dist/expressjs.d.ts",
      "import": "./dist/expressjs.js"
    },
    "./nextjs": {
      "types": "./dist/nextjs.d.ts",
      "import": "./dist/nextjs.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "publint": "publint",
    "build": "vite build"
  },
  "peerDependencies": {
    "express": ">=5.0.0",
    "next": ">=15.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/react": "^19.0.0",
    "@types/supertest": "^6.0.3",
    "express": "^5.1.0",
    "next": "^15.0.0",
    "publint": "^0.3.15",
    "supertest": "^7.1.4",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "iron-session": "^8.0.4"
  }
}