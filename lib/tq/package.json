{
  "name": "@supergrowthai/tq",
  "version": "1.0.9",
  "description": "Task queue management library with multiple executor types and async task handling",
  "repository": "git+https://github.com/captadexp/next-blog.git",
  "homepage": "https://github.com/captadexp/next-blog/tree/main#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.mjs"
      },
      "require": {
        "types": "./dist/types.d.cts",
        "default": "./dist/types.cjs"
      }
    },
    "./adapters": {
      "import": {
        "types": "./dist/adapters/index.d.ts",
        "default": "./dist/adapters/index.mjs"
      },
      "require": {
        "types": "./dist/adapters/index.d.cts",
        "default": "./dist/adapters/index.cjs"
      }
    },
    "./core/base/interfaces": {
      "import": {
        "types": "./dist/core/base/interfaces.d.ts",
        "default": "./dist/core/base/interfaces.mjs"
      },
      "require": {
        "types": "./dist/core/base/interfaces.d.cts",
        "default": "./dist/core/base/interfaces.cjs"
      }
    },
    "./core/actions": {
      "import": {
        "types": "./dist/core/Actions.d.ts",
        "default": "./dist/core/Actions.mjs"
      },
      "require": {
        "types": "./dist/core/Actions.d.cts",
        "default": "./dist/core/Actions.cjs"
      }
    },
    "./core/actions/async": {
      "import": {
        "types": "./dist/core/async/AsyncActions.d.ts",
        "default": "./dist/core/async/AsyncActions.mjs"
      },
      "require": {
        "types": "./dist/core/async/AsyncActions.d.cts",
        "default": "./dist/core/async/AsyncActions.cjs"
      }
    },
    "./core/async": {
      "import": {
        "types": "./dist/core/async/AsyncTaskManager.d.ts",
        "default": "./dist/core/async/AsyncTaskManager.mjs"
      },
      "require": {
        "types": "./dist/core/async/AsyncTaskManager.d.cts",
        "default": "./dist/core/async/AsyncTaskManager.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils/task-id-gen.d.ts",
        "default": "./dist/utils/task-id-gen.mjs"
      },
      "require": {
        "types": "./dist/utils/task-id-gen.d.cts",
        "default": "./dist/utils/task-id-gen.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "vite build --watch",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "publint": "publint",
    "test": "bun src/test",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "author": "Capt ADExp <tech@developer.toys>",
  "license": "MIT",
  "dependencies": {
    "@supergrowthai/mq": "workspace:*",
    "lodash": "^4.17.23",
    "moment": "^2.30.1",
    "mongodb": "^7.0.0"
  },
  "devDependencies": {
    "@prisma/client": "^7.2.0",
    "@supergrowthai/utils": "workspace:*",
    "@types/bun": "^1.3.7",
    "@types/node": "^25.0.10",
    "publint": "^0.3.17",
    "typescript": "^5.9.2",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "memoose-js": "2.0.4"
  }
}