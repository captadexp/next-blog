{
  "name": "@supergrowthai/next-blog-types",
  "version": "1.0.2",
  "description": "Shared TypeScript types for SupergrowthAI Next Blog platform",
  "repository": "git+https://github.com/captadexp/next-blog.git",
  "homepage": "https://github.com/captadexp/next-blog/tree/main#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.mjs"
      },
      "require": {
        "types": "./dist/server.d.cts",
        "default": "./dist/server.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client.d.ts",
        "default": "./dist/client.mjs"
      },
      "require": {
        "types": "./dist/client.d.cts",
        "default": "./dist/client.cjs"
      }
    },
    "./plugin/client": {
      "import": {
        "types": "./dist/plugin/client.d.ts",
        "default": "./dist/plugin/client.mjs"
      },
      "require": {
        "types": "./dist/plugin/client.d.cts",
        "default": "./dist/plugin/client.cjs"
      }
    },
    "./plugin/server": {
      "import": {
        "types": "./dist/plugin/server.d.ts",
        "default": "./dist/plugin/server.mjs"
      },
      "require": {
        "types": "./dist/plugin/server.d.cts",
        "default": "./dist/plugin/server.cjs"
      }
    },
    "./plugin/manifest": {
      "import": {
        "types": "./dist/plugin/manifest.d.ts",
        "default": "./dist/plugin/manifest.mjs"
      },
      "require": {
        "types": "./dist/plugin/manifest.d.cts",
        "default": "./dist/plugin/manifest.cjs"
      }
    },
    "./sdk/client": {
      "import": {
        "types": "./dist/sdk/client.d.ts",
        "default": "./dist/sdk/client.mjs"
      },
      "require": {
        "types": "./dist/sdk/client.d.cts",
        "default": "./dist/sdk/client.cjs"
      }
    },
    "./sdk/server": {
      "import": {
        "types": "./dist/sdk/server.d.ts",
        "default": "./dist/sdk/server.mjs"
      },
      "require": {
        "types": "./dist/sdk/server.d.cts",
        "default": "./dist/sdk/server.cjs"
      }
    },
    "./database/entities": {
      "import": {
        "types": "./dist/database/entities.d.ts",
        "default": "./dist/database/entities.mjs"
      },
      "require": {
        "types": "./dist/database/entities.d.cts",
        "default": "./dist/database/entities.cjs"
      }
    },
    "./database/adapter": {
      "import": {
        "types": "./dist/database/adapter.d.ts",
        "default": "./dist/database/adapter.mjs"
      },
      "require": {
        "types": "./dist/database/adapter.d.cts",
        "default": "./dist/database/adapter.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "vite build --watch",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "publint": "publint"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^24.5.2",
    "publint": "^0.3.15",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "memoose-js": "2.0.2"
  },
  "author": "Capt ADExp <tech@developer.toys>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}