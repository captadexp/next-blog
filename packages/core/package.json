{
  "name": "@supergrowthai/next-blog",
  "version": "2.0.0",
  "description": "Add blogging to your nextjs project in a jiffy",
  "repository": "https://github.com/captadexp/next-blog",
  "homepage": "https://github.com/captadexp/next-blog/tree/main#readme",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite build --watch",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "react",
    "cms",
    "blogging",
    "headless-cms",
    "next-js",
    "secure"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": {
        "types": "./dist/adapters/index.d.ts",
        "default": "./dist/adapters/index.js"
      },
      "require": {
        "types": "./dist/adapters/index.d.ts",
        "default": "./dist/adapters/index.js"
      },
      "default": "./dist/adapters/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "./dist/index.js",
  "author": "Capt ADExp <tech@developer.toys>",
  "license": "ISC",
  "dependencies": {
    "fast-xml-parser": "^5.3.0",
    "moment": "^2.30.1"
  },
  "optionalDependencies": {
    "mongodb": "^6.17.0",
    "uuid": "^11.1.0",
    "sift": "^17.1.3",
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/lib-storage": "^3.903.0",
    "@xmldom/xmldom": "^0.8.11"
  },
  "peerDependencies": {
    "next": "^15.2.4"
  },
  "devDependencies": {
    "@supergrowthai/next-blog-dashboard": "workspace:*",
    "@supergrowthai/types": "workspace:*",
    "vite-plugin-static-copy": "^3.1.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^24.5.2",
    "@types/uuid": "^10.0.0",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4"
  }
}