{
  "name": "@supergrowthai/next-blog",
  "version": "2.0.0-beta.7",
  "description": "Add blogging to your nextjs project in a jiffy",
  "repository": "https://github.com/captadexp/next-blog",
  "homepage": "https://github.com/captadexp/next-blog/tree/main#readme",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist && mkdir -p dist/core",
    "dev": "npm run clean && vite build --watch",
    "build": "npm run clean && vite build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "react",
    "cms",
    "blogging",
    "headless-cms",
    "next-js",
    "secure"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": {
        "types": "./dist/adapters/index.d.ts",
        "default": "./dist/adapters/index.js"
      },
      "require": {
        "types": "./dist/adapters/index.d.ts",
        "default": "./dist/adapters/index.js"
      },
      "default": "./dist/adapters/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "./dist/index.js",
  "author": "Capt ADExp <tech@developer.toys>",
  "license": "ISC",
  "dependencies": {
    "moment": "^2.30.1"
  },
  "optionalDependencies": {
    "mongodb": "^6.17.0",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "next": "^15.3.5"
  },
  "devDependencies": {
    "vite-plugin-static-copy": "^3.1.0",
    "@supergrowthai/next-blog-dashboard": "workspace:*",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^22.16.0",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.1",
    "vite-plugin-dts": "^4.5.4"
  }
}