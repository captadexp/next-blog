{
  "name": "@supergrowthai/next-blog",
  "version": "3.0.1-beta.2",
  "description": "Add blogging to your nextjs project in a jiffy",
  "repository": "git+https://github.com/captadexp/next-blog.git",
  "homepage": "https://github.com/captadexp/next-blog/tree/main#readme",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite build --watch",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "publint": "publint",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "react",
    "cms",
    "blogging",
    "headless-cms",
    "next-js",
    "secure"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js",
      "default": "./dist/adapters/index.js"
    },
    "./next": {
      "types": "./dist/nextjs/index.d.ts",
      "import": "./dist/nextjs/index.js",
      "default": "./dist/nextjs/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "./dist/index.js",
  "author": "Capt ADExp <tech@developer.toys>",
  "license": "MIT",
  "dependencies": {
    "memoose-js": "2.0.2"
  },
  "optionalDependencies": {
    "fast-xml-parser": "^5.3.2",
    "mongodb": "^7.0.0",
    "uuid": "^13.0.0",
    "sift": "^17.1.3",
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/lib-storage": "^3.948.0",
    "@xmldom/xmldom": "^0.8.11"
  },
  "peerDependencies": {
    "next": ">=15.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@supergrowthai/next-blog-dashboard": "workspace:*",
    "@supergrowthai/next-blog-types": "workspace:*",
    "@supergrowthai/oneapi": "workspace:*",
    "@supergrowthai/utils": "workspace:*",
    "@tailwindcss/vite": "^4.1.17",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^25.0.0",
    "@types/uuid": "^11.0.0",
    "@types/bun": "^1.3.1",
    "mongodb-memory-server": "^10.4.1",
    "next": "^16.0.8",
    "publint": "^0.3.16",
    "vite": "^7.2.7",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.1"
  }
}