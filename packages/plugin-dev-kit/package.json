{
  "name": "@supergrowthai/plugin-dev-kit",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "next-blog": "./bin/cli.ts",
    "create-next-blog-plugin": "./bin/create.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "default": "./dist/client/index.js"
    },
    "./content": {
      "types": "./dist/content/index.d.ts",
      "import": "./dist/content/index.js",
      "default": "./dist/content/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./build": {
      "types": "./dist/build/index.d.ts",
      "import": "./dist/build/index.js",
      "default": "./dist/build/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite build --watch",
    "build": "vite build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@supergrowthai/jsx-runtime": "workspace:*",
    "@supergrowthai/types": "workspace:*",
    "@vitejs/plugin-react": "^5.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "esbuild": "^0.24.2",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "bun-types": "^1.2.22",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "vite": "^7.0.0"
  }
}