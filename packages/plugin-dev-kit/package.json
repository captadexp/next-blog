{
  "name": "@supergrowthai/plugin-dev-kit",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "next-blog": "./bin/cli.ts",
    "create-next-blog-plugin": "./bin/create.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "default": "./dist/client/index.js"
    },
    "./content": {
      "types": "./dist/content/index.d.ts",
      "import": "./dist/content/index.js",
      "default": "./dist/content/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./build": {
      "types": "./dist/build/index.d.ts",
      "import": "./dist/build/index.js",
      "default": "./dist/build/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js",
      "default": "./dist/jsx-dev-runtime.js"
    },
    "./client/test": {
      "types": "./dist/client/test.d.ts",
      "import": "./dist/client/test.js",
      "default": "./dist/client/test.js"
    },
    "./server/test": {
      "types": "./dist/server/test.d.ts",
      "import": "./dist/server/test.js",
      "default": "./dist/server/test.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite build --watch",
    "build": "vite build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@supergrowthai/jsx-runtime": "workspace:*",
    "@supergrowthai/next-blog-types": "workspace:*",
    "@supergrowthai/utils": "workspace:*",
    "@vitejs/plugin-react": "^5.1.2",
    "chalk": "^5.3.0",
    "commander": "^14.0.2",
    "esbuild": "^0.27.2",
    "glob": "^13.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.9",
    "postcss": "^8.4.49",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "tailwindcss": ">=4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "vite": "^7.3.1"
  },
  "peerDependenciesMeta": {
    "tailwindcss": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}