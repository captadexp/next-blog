{
  "name": "@supergrowthai/mq",
  "version": "1.0.1",
  "description": "Message queue library with multiple provider backends (Kinesis, MongoDB, Memory, Immediate)",
  "repository": "https://github.com/captadexp/next-blog",
  "homepage": "https://github.com/captadexp/next-blog/tree/main#readme",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./core/types": {
      "types": "./dist/core/types.d.ts",
      "import": "./dist/core/types.mjs",
      "require": "./dist/core/types.js"
    },
    "./core/utils": {
      "types": "./dist/core/utils.d.ts",
      "import": "./dist/core/utils.mjs",
      "require": "./dist/core/utils.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.mjs",
      "require": "./dist/adapters/index.js"
    },
    "./queues": {
      "types": "./dist/queues/index.d.ts",
      "import": "./dist/queues/index.mjs",
      "require": "./dist/queues/index.js"
    },
    "./queues/memory": {
      "types": "./dist/queues/implementations/memory.d.ts",
      "import": "./dist/queues/implementations/memory.mjs",
      "require": "./dist/queues/implementations/memory.js"
    },
    "./queues/immediate": {
      "types": "./dist/queues/implementations/immediate.d.ts",
      "import": "./dist/queues/implementations/immediate.mjs",
      "require": "./dist/queues/implementations/immediate.js"
    },
    "./queues/mongodb": {
      "types": "./dist/queues/implementations/mongodb.d.ts",
      "import": "./dist/queues/implementations/mongodb.mjs",
      "require": "./dist/queues/implementations/mongodb.js"
    },
    "./queues/kinesis": {
      "types": "./dist/queues/implementations/kinesis.d.ts",
      "import": "./dist/queues/implementations/kinesis.mjs",
      "require": "./dist/queues/implementations/kinesis.js"
    },
    "./queues/kinesis/*": {
      "types": "./dist/queues/implementations/kinesis/*.d.ts",
      "import": "./dist/queues/implementations/kinesis/*.mjs",
      "require": "./dist/queues/implementations/kinesis/*.js"
    },
    "./shard": {
      "types": "./dist/shard/index.d.ts",
      "import": "./dist/shard/index.mjs",
      "require": "./dist/shard/index.js"
    },
    "./shard/lock-providers": {
      "types": "./dist/shard/lock-providers/index.d.ts",
      "import": "./dist/shard/lock-providers/index.mjs",
      "require": "./dist/shard/lock-providers/index.js"
    },
    "./shard/leaser": {
      "types": "./dist/shard/leaser/index.d.ts",
      "import": "./dist/shard/leaser/index.mjs",
      "require": "./dist/shard/leaser/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "vite build --watch",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "author": "Capt ADExp <tech@developer.toys>",
  "license": "MIT",
  "dependencies": {
    "ioredis": "^5.6.1",
    "memoose-js": "^1.1.2",
    "mongodb": "^6.16.0",
    "moment": "^2.30.1",
    "bson": "^6.10.3",
    "@aws-sdk/client-kinesis": "^3.797.0"
  },
  "devDependencies": {
    "@supergrowthai/utils": "workspace:*",
    "@types/bun": "^1.3.1",
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}