{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "declaration": true,
    "declarationMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strict": true,
    "allowImportingTsExtensions": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "target": "es2020",
    "noEmit": true,
    "outDir": "dist",
    "types": [
      "bun"
    ],
    "paths": {
    },
    "lib": [
      "esnext"
    ],
    "rootDir": "src"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "dist"
  ]
}
